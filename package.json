    "vite": "^2.8.0",
    "vite-node": "^0.7.10",
    "vite-plugin-cloudflare": "^0.1.0",
    "vite-raw-plugin": "^1.0.1",
    "vitest": "^0.7.10"
  },
  "dependencies": {
    "opentype.js": "^1.3.4"
  },
  "engines": {
    "node": ">=16.7"
  },
  "packageManager": "pnpm"
